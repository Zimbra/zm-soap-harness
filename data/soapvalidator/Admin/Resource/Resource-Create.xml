<t:property name="server.zimbraAdmin" value="${zimbraServer.name}"/>
<t:test_case testcaseid="AcctSetup01_resource_create" type="always" >
    <t:objective>basic system check</t:objective>

    <t:test id="ping" required="true">
        <t:request>
            <PingRequest xmlns="urn:zimbraAdmin"/>
        </t:request>
        <t:response>
            <t:select path="//admin:PingResponse"/>
        </t:response>
    </t:test>

    <t:test id="admin_login" required="true" >
        <t:request>
            <AuthRequest xmlns="urn:zimbraAdmin">
                <name>${admin.user}</name>
                <password>${admin.password}</password>
            </AuthRequest>
        </t:request>
        <t:response>
            <t:select path="//admin:AuthResponse/admin:authToken" set="authToken"/>
        </t:response>
    </t:test>

    <t:test>
        <t:request>
            <CreateCosRequest xmlns="urn:zimbraAdmin">
                <name xmlns="">TestCos${TIME}${COUNTER}</name>        
            </CreateCosRequest>
        </t:request>
        <t:response>
            <t:select path="//admin:CreateCosResponse/admin:cos" attr="id" set="cos.id"/>
        </t:response>
    </t:test>

</t:test_case>


<t:test_case testcaseid="CreateCalendarResource01" type="bhr">
    <t:objective>  Create a resource with type= Equipment  and with all valid values  </t:objective>

    <t:test>
        <t:request>
			<CreateCalendarResourceRequest xmlns="urn:zimbraAdmin">
				<name>${Resource.name1}</name>
				<a n="zimbraCOSId">${cos.id}</a>
				<a n="zimbraCalResType">${Resource.equipment}</a>
				<a n="zimbraAccountStatus">${active}</a>
				<a n="displayName">${displayName1}</a>
				<a n="description">${description}</a>
				<a n="zimbraNotes">${zimbraNotes}</a>
				<a n="zimbraCalResSite">t</a>
				<a n="zimbraCalResLocationDisplayName">${Resource.name1} ${displayName1}</a>
				<a n="zimbraCalResBuilding">${building}</a>
				<a n="zimbraCalResFloor">${floor}</a>
				<a n="zimbraCalResRoom">${room}</a>
				<a n="street">${street}</a>
				<a n="l">${city}</a>
				<a n="st">${state}</a>
				<a n="co">${country}</a>
				<a n="postalCode">${postalcode}</a>
				<a n="zimbraCalResAutoAcceptDecline">${globals.true}</a>
				<a n="zimbraCalResAutoDeclineIfBusy">${globals.false}</a>
			</CreateCalendarResourceRequest>
        </t:request>
        <t:response>
            <t:select path="//admin:CreateCalendarResourceResponse"/>
        </t:response>
    </t:test>

</t:test_case>


