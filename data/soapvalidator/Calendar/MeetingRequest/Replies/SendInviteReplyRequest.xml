<t:test_case testcaseid="SendInviteReplyRequest_02" type="bhr" areas="Calendar" bugids="38152">
    <t:objective>Verify responses are reconciled if personal name does not match on invitation/reply</t:objective>
    <t:steps>
    1. Login as organizer
    2. Create a simple meeting request.  Invite the invitee using "Foo &lt;foo@example.com&gt;".
    3. Login as the invitee
    4. RSVP (Accept) the meeting.  Send from "Bar &lt;foo@example.com&gt;".
    5. Login as organizer
    6. Verify the attendee status is AC.  No additional attendee should be added (i.e. Bar should not be added as a second attendee)
    </t:steps>



	<t:property name="server.zimbraAccount" value="${organizer6.server}"/>

    <t:test id="auth1" required="true">
        <t:request>
            <AuthRequest xmlns="urn:zimbraAccount">
                <account by="name">${organizer6.name}</account>
                <password>${defaultpassword.value}</password>
            </AuthRequest>
        </t:request>
        <t:response>
            <t:select path="//acct:AuthResponse/acct:authToken" set="authToken"/>
        </t:response>
    </t:test>
   
   
   
	<t:property name="appointment.subject" value="Subject of meeting${TIME}.${COUNTER}"/>
	<t:property name="appointment.location" value="Location of meeting${TIME}.${COUNTER}"/>
	<t:property name="appointment.content" value="Content of the message${TIME}.${COUNTER}"/>

	<t:property name="time.20180101120000.gmt" value="1514808000000"/>
	<t:property name="time.20180101120000.pst" value="${TIME(-8h)[${time.20180101120000.gmt}]}"/>

    <t:test>
        <t:request>
			<CreateAppointmentRequest xmlns="urn:zimbraMail">
				<m >
					<inv>
						<comp status="CONF" fb="B" transp="O" allDay="0" name="${appointment.subject}">
							<or a="${organizer6.name}"/>
							<at a="${invitee6.name}" d="Foo" role="REQ" ptst="NE" rsvp="1"/>
							<s d="${ICALTIME[${time.20180101120000.pst}]}" tz="${timezone.pst}" />
							<e  d="${ICALTIME(+3h)[${time.20180101120000.pst}]}" tz="${timezone.pst}" />
						</comp>
					</inv>
					<e a="${invitee6.name}" t="t" p="Foo"/>
					<su>${appointment.subject}</su>
					<mp ct="text/plain">
						<content>${appointment.content}</content>
					</mp>
				</m>
			</CreateAppointmentRequest>
        </t:request>
        <t:response>
            <t:select path="//mail:CreateAppointmentResponse">
            	<t:select attr="calItemId" set="organizer6.appt1.calItemId"/>
            	<t:select attr="invId" set="organizer6.appt1.invId"/>
            	<t:select attr="apptId" set="organizer6.appt1.apptId"/>
            </t:select>
